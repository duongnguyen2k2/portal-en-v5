@model API.Areas.Admin.Models.DMCoQuan.DMCoQuanModel
@{
    if (Model.Item.Id == 0)
    {
        ViewData["Title"] = "Thêm mới Cơ Quan";
    }
    else
    {
        ViewData["Title"] = "Cập nhật Cơ Quan";
    }
    //string ControllerName = this.ViewContext.RouteData.Values["controller"].ToString();
    string ControllerName = "DMCoQuan";

    string ClsCulturVI = "active show";
    string ClsCulturEN = "";
    string ClsCulturConfig = "";
    if (Model.Item.Culture == "en")
    {
        ClsCulturVI = "";
        ClsCulturEN = "active show";
        ClsCulturConfig = "";
    }else if (Model.Item.Culture == "vi")
    {
        ClsCulturVI = "active show";
        ClsCulturEN = "";
        ClsCulturConfig = "";
    }else if (Model.Item.Culture == "config")
    {
        ClsCulturVI = "";
        ClsCulturEN = "";
        ClsCulturConfig = "active show";
    }
   

}

<div class="card">
    <div class="card-header">
        <h4>@ViewData["Title"]</h4>
    </div>
